{
  "manifest_version": 2,
  "name": "El bromercio chrome extension",
  "version": "1.1.6",
  "description": "Project to access news from popular peruvian newspapers (pink and yellow)",
  "author": "jorgeluisyh",
  "permissions": [
    "activeTab"
  ],
    "browser_action":{
    "default_popup":"popup.html"
  },
  "icons": { "16": "unlock128.png",
    "48": "unlock128.png",
   "128": "unlock128.png" },

   "content_scripts": [{
    "matches": ["https://elcomercio.pe/*","https://gestion.pe/*"],
    "css": ["/assets/css/sweetalert2.min.css"],
    "js": ["/assets/js/sweetalert2.all.min.js", "content.js"]
}]
}