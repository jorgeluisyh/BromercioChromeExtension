{
  "manifest_version": 2,
  "name": "El bromercio chrome extension",
  "version": "1.0",
  "description": "Project to access news from popular peruvian newspapers (pink and yellow)",
  "author": "Fsociety",
  "permissions": [

    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_popup":"popup.html"
  },
  "icons": { "16": "unlock128.png",
    "48": "unlock128.png",
   "128": "unlock128.png" },

   "content_scripts": [{
    "matches": ["https://elcomercio.pe/*"],
    "js": ["content.js"]
}]
}